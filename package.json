{
  "name": "gtech_project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:overview": "node scripts/test-overview.cjs",
    "test:full": "node scripts/test-summary.cjs",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:summary": "node scripts/test-overview.cjs",
    "docker:build": "docker build -t ai-code-analyzer .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t ai-code-analyzer-dev .",
    "docker:run": "docker run -p 3000:80 ai-code-analyzer",
    "docker:run-dev": "docker run -p 5173:5173 -v $(pwd):/app ai-code-analyzer-dev",
    "docker:compose": "docker-compose up",
    "docker:compose-dev": "docker-compose --profile dev up",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
